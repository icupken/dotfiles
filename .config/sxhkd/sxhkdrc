super + e
	st
# program launcher
super + r
	.config/rofi/bin/launcher_colorful	
#Power Menu
super + x
	 .config/rofi/bin/powermenu
# make sxhkd reload its configuration files:
super + Escape
	pkill -USR1 -x sxhkd
# Volume Control
XF86Audio{LowerVolume,RaiseVolume}
	amixer set Master 5%{-,+}
XF86AudioMute
  amixer -q sset Master toggle
